{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Plasticity Properties",
  "type": "object",
  "properties": {
    "yield_strength": {
      "type": "number",
      "description": "Stress at yield point (Pascals)",
      "minimum": 0
    },
    "hardening_coefficient": {
      "type": "number",
      "description": "Material hardening coefficient (dimensionless)",
      "minimum": 0
    },
    "plastic_strain": {
      "type": "number",
      "description": "Amount of permanent strain (dimensionless)",
      "minimum": 0
    }
  },
  "required": ["yield_strength"]
}
