{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Magnetic Force",
  "type": "object",
  "properties": {
    "type": { "const": "magnetic" },
    "charge": { "type": "number" },
    "velocity": { "type": "number" },
    "magnetic_field": { "type": "number" },
    "angle": { "type": "number", "description": "Angle between velocity and magnetic field (degrees)" }
  },
  "required": ["type", "charge", "velocity", "magnetic_field", "angle"]
}
