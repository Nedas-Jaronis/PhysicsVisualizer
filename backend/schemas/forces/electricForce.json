{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Electric Force",
  "type": "object",
  "properties": {
    "type": { "const": "electric" },
    "charge1": { "type": "number" },
    "charge2": { "type": "number" },
    "distance": { "type": "number" },
    "k_e": { "type": "number", "default": 8.99e9 },
    "source": {
      "type": "string",
      "description": "The object exerting the electric force"
    },
    "object1_id": {
      "type": "string",
      "description": "ID of the first object involved"
    },
    "object2_id": {
      "type": "string",
      "description": "ID of the second object involved"
    }
  },
  "required": ["type", "charge1", "charge2", "distance", "object1_id", "object2_id"]
}
